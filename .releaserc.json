{
  "repositoryUrl": "https://github.com/gary-Shen/release-test.git",
  "tagFormat": "s3-browser@v${version}",
  "branches": [
    "main"
  ],
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "angular",
        "releaseRules": [
          {
            "type": "feat",
            "release": "minor",
            "scope": "s3-browser"
          },
          {
            "type": "fix",
            "release": "patch",
            "scope": "s3-browser"
          },
          {
            "type": "perf",
            "release": "patch",
            "scope": "s3-browser"
          },
          {
            "type": "revert",
            "release": "patch",
            "scope": "s3-browser"
          },
          {
            "type": "docs",
            "release": "patch",
            "scope": "s3-browser"
          },
          {
            "type": "style",
            "release": "patch",
            "scope": "s3-browser"
          },
          {
            "type": "refactor",
            "release": "patch",
            "scope": "s3-browser"
          },
          { "type": "*", "scope": "!s3", "release": false },
          { "type": "*", "scope": "!s3-browser", "release": false }
        ]
      }
    ],
    [
      "@semantic-release/release-notes-generator",
      {
        "preset": "conventionalcommits",
        "writerOpts": {
          "commitsSort": ["subject", "scope"]
        },
        "presetConfig": {
          "types": [
            {
              "type": "refactor",
              "scope": "s3-browser",
              "hidden": false,
              "section": "Code Refactoring"
            },
            {
              "type": "fix",
              "scope": "s3-browser",
              "hidden": false,
              "section": "Bug fixes"
            },
            {
              "type": "feat",
              "scope": "s3-browser",
              "hidden": false,
              "section": "Features"
            },
            {
              "type": "style",
              "scope": "s3-browser",
              "hidden": false,
              "section": "Styling"
            },
            {
              "type": "docs",
              "scope": "s3-browser",
              "hidden": false,
              "section": "Documentation"
            },
            {
              "type": "perf",
              "scope": "s3-browser",
              "hidden": false,
              "section": "Performance Improvements"
            },
            {
              "type": "revert",
              "scope": "s3-browser",
              "hidden": false,
              "section": "Reverts"
            }
          ]
        }
      }
    ],
    [
      "@semantic-release/github"
    ]
  ]
}