{
    "repositoryUrl": "https://github.com/gary-Shen/release-test.git",
    "tagFormat": "s3-browser@v${version}",
    "branches": [
      "main"
    ],
    "plugins": [
      ["@semantic-release/commit-analyzer", {
        "preset": "conventionalcommits",
        "releaseRules": [
          {"type": "feat", "release": "minor", "scope": "s3-browser"},
          {"type": "fix", "release": "patch", "scope": "s3-browser"},
          {"type": "perf", "release": "patch", "scope": "s3-browser"},
          {"type": "revert", "release": "patch", "scope": "s3-browser"},
          {"type": "docs", "release": "patch", "scope": "s3-browser"},
          {"type": "style", "release": "patch", "scope": "s3-browser"},
          {"type": "refactor", "release": "patch", "scope": "s3-browser"},
          {"type": "feat", "release": false },
          {"type": "fix", "release": false },
          {"type": "perf", "release": false },
          {"type": "revert", "release": false },
          {"type": "docs", "release": false },
          {"type": "style", "release": false },
          {"type": "refactor", "release": false }
        ]
      }],
      ["@semantic-release/release-notes-generator", {
        "preset": "conventionalcommits",
        "presetConfig": {
            "types": [
                {
                    "type": "fix",
                    "scope": "s3-browser",
                    "hidden": false,
                    "section": "Bug fixes"
                },
                {
                    "type": "feat",
                    "scope": "s3-browser",
                    "hidden": false,
                    "section": "Features"
                },
                {
                    "type": "refactor",
                    "scope": "s3-browser",
                    "hidden": false,
                    "section": "Code Refactoring"
                }
            ]
        }
      }],
      ["@semantic-release/github"]
    ]
  }
  