{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug semantic-release",
      "runtimeExecutable": "${workspaceFolder}/setup.sh",
      "program": "${workspaceFolder}/node_modules/.bin/semantic-release",
      "args": ["--dry-run"],
      "env": {
        "DEBUG": "*",
        "GH_TOKEN": "YOUR_GITHUB"
      }
    }
  ]
}
